<launch>
	<include file="$(find source_control)/launch/single_catvehicle_skidpan.launch"/>
	<node name="gzclient" pkg="gazebo_ros" type="gzclient" output="screen"/>
	<node name="rviz" pkg="rviz" type="rviz" args="-d $(find source_control)/config/catvehicle_startup.rviz" output="screen"/>
	<node name="mobot_gazebo_state" pkg="mobot_gazebo_state" type="mobot_gazebo_state" output="screen"/>
	<node name="hoffmann_controller" pkg="hoffmann_controller" type="hoffmann_controller" output="screen"/>
</launch>